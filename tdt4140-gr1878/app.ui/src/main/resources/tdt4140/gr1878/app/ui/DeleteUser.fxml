<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane styleClass="pane" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="20.0" layoutY="40.0" prefWidth="400.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="40.0">
         <children>
            <VBox>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </VBox>
            <VBox>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
               <children>
                  <Label fx:id="infoLabelDeleteUser" />
               </children>
            </VBox>
            <VBox fx:id="vboxTech">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </VBox>
            <TextField fx:id="usernameField" promptText="Username" />
            <VBox prefHeight="7.0" prefWidth="100.0" />
            <PasswordField fx:id="passwordField" promptText="Password" />
            <VBox prefHeight="7.0" prefWidth="100.0" />
            <PasswordField fx:id="passwordField1" promptText="Verify password" />
            <VBox prefHeight="7.0" prefWidth="100.0" />
            <Button fx:id="deleteUser" mnemonicParsing="false" onAction="#deleteUser" text="Delete user" />
            <VBox prefHeight="7.0" prefWidth="100.0" />
            <Button fx:id="goBack" mnemonicParsing="false" onAction="#goBack" text="Back" />
         </children>
      </VBox>
      <Label layoutX="20.0" layoutY="14.0" prefHeight="48.0" prefWidth="248.0" styleClass="headerLabel" text="Delete User">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
