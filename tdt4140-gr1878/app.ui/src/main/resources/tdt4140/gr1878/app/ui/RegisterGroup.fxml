<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane styleClass="pane" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox styleClass="hbox" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="groupsButton" mnemonicParsing="false" onAction="#groupsButton" styleClass="menuButton" text="Groups" />
            <Button fx:id="profileButton" mnemonicParsing="false" onAction="#profileButton" styleClass="menuButton" text="Profile" />
            <Button fx:id="activityButton" mnemonicParsing="false" onAction="#activityButton" styleClass="menuButton" text="Activity" />
            <Button fx:id="settingsButton" mnemonicParsing="false" onAction="#settingsButton" styleClass="menuButton" text="Information" />
         </children>
      </HBox>
      <VBox layoutX="14.0" layoutY="20.0" />
      <VBox layoutX="14.0" layoutY="30.0" />
      <VBox fx:id="vboxTech" layoutX="14.0" layoutY="40.0" />
      <Label fx:id="MyGroups" layoutX="50.0" layoutY="40.0" prefHeight="50.0" prefWidth="263.0" styleClass="headerLabel" text="Create new group" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="331.0" layoutY="430.0" prefHeight="60.0" prefWidth="226.0" text="activityTrace">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" layoutX="200.0" layoutY="125.0" promptText="..." />
      <TextField fx:id="pointsField" layoutX="200.0" layoutY="165.0" promptText="..." />
      <TextField fx:id="prizeField" layoutX="200.0" layoutY="205.0" promptText="..." />
      <TextField fx:id="memberField" layoutX="200.0" layoutY="246.0" prefHeight="23.0" prefWidth="167.0" promptText="Username 1,Username 2, Username 3...Username 1,Username 2, Username 3...">
         <font>
            <Font size="8.5" />
         </font>
      </TextField>
      <Label layoutX="72.0" layoutY="130.0" text="Group name:" />
      <Label layoutX="81.0" layoutY="170.0" text="Points goal:" />
      <Label layoutX="62.0" layoutY="183.0" text="(2/km running, 1/km biking)">
         <font>
            <Font size="7.0" />
         </font>
      </Label>
      <Label layoutX="63.0" layoutY="249.0" text="Add members:" />
      <Button fx:id="createButton" layoutX="239.0" layoutY="283.0" mnemonicParsing="false" onAction="#createButton" text="Create the group!" />
      <Label fx:id="infoLabel" layoutX="331.0" layoutY="301.0" />
      <Label layoutX="117.0" layoutY="212.0" text="Prize:" />
      <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButton" styleClass="backButton" text="Back" />
   </children>
</AnchorPane>
