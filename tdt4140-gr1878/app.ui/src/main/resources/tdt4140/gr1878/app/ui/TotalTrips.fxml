<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane styleClass="pane" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="20.0" layoutY="14.0" prefHeight="48.0" prefWidth="248.0" styleClass="headerLabel" text="Total Trips">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <HBox styleClass="hbox" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="profileButton" mnemonicParsing="false" onAction="#MyProfileDataCollectorController" styleClass="menuButton" text="Profile" />
            <Button fx:id="activityButton" mnemonicParsing="false" onAction="#activityButton" styleClass="menuButton" text="Activity" />
            <Button fx:id="trondheimButton" mnemonicParsing="false" onAction="#trondheimButton" styleClass="menuButton" text="Trondheim" />
            <Button fx:id="totaltripsButton" mnemonicParsing="false" onAction="#totaltripsButton" styleClass="menuButton" text="Total Trips" />
         </children>
      </HBox>
      <Label fx:id="biketrips" layoutX="31.0" layoutY="106.0" text="Bike Trips:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="runs" layoutX="87.0" layoutY="179.0" text="Runs:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="runsShow" layoutX="163.0" layoutY="179.0" prefHeight="29.0" prefWidth="218.0" text="  -">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="biketripShow" layoutX="163.0" layoutY="106.0" prefHeight="29.0" prefWidth="218.0" text="  -">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="updateButton" layoutX="90.0" layoutY="234.0" mnemonicParsing="false" onAction="#updateTrips" text="Update" />
   </children>
</AnchorPane>
