<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane styleClass="pane" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="result" layoutX="39.0" layoutY="212.0" prefHeight="87.0" prefWidth="395.0" translateX="70.0">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <VBox layoutX="27.0" layoutY="14.0" prefHeight="484.0" prefWidth="536.0">
         <children>
            <VBox fx:id="vboxTech" prefHeight="0.0" prefWidth="400.0">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </VBox>
            <HBox prefHeight="269.0" prefWidth="531.0">
               <children>
                  <HBox prefHeight="112.0" prefWidth="152.0" translateX="75.0" translateY="100.0">
                     <children>
                        <Button fx:id="runningButton" mnemonicParsing="false" onAction="#runningButton" prefHeight="85.0" prefWidth="150.0" text="Run">
                           <font>
                              <Font size="21.0" />
                           </font>
                        </Button>
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="150.0" translateX="145.0" translateY="100.0">
                     <children>
                        <Button fx:id="bicycleButton" mnemonicParsing="false" onAction="#bicycleButton" prefHeight="87.0" prefWidth="150.0" text="Bike">
                           <font>
                              <Font size="21.0" />
                           </font>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </HBox>
            <Label fx:id="chooseActivity" />
            <VBox prefHeight="152.0" prefWidth="531.0" translateY="30.0" />
            <Button fx:id="trackingButton" mnemonicParsing="false" onAction="#trackingButton" prefHeight="150.0" prefWidth="366.0" text="Start workout!" translateX="80.0" translateY="-70.0">
               <font>
                  <Font size="35.0" />
               </font>
            </Button>
            <Label prefHeight="41.0" prefWidth="222.0" text="activityTrace">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <HBox prefHeight="51.0" prefWidth="536.0">
               <children>
                  <HBox fx:id="addPointsBox" prefHeight="27.0" prefWidth="321.0">
                     <children>
                        <HBox prefHeight="27.0" prefWidth="203.0" />
                        <Label fx:id="chooseGroupLabel" prefHeight="26.0" prefWidth="160.0" text="Add points to group:" />
                     </children>
                  </HBox>
                  <TextField fx:id="groupnamePoints" prefHeight="27.0" prefWidth="140.0" promptText="Groupname" />
                  <HBox prefHeight="27.0" prefWidth="7.0" />
                  <Button fx:id="addPointsButton" mnemonicParsing="false" onAction="#addPointsButton" prefHeight="26.0" prefWidth="62.0" text="Add" />
               </children>
            </HBox>
         </children>
      </VBox>
      <Label fx:id="timerLabel" layoutX="185.0" layoutY="70.0" prefHeight="55.0" prefWidth="205.0" text="00:00:00" textAlignment="RIGHT">
         <font>
            <Font size="44.0" />
         </font>
      </Label>
      <Button fx:id="mapButton" layoutX="182.0" layoutY="53.0" mnemonicParsing="false" onAction="#mapButton" prefHeight="66.0" prefWidth="206.0" text="Show workout in map">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="goBack" mnemonicParsing="false" onAction="#goBack" styleClass="backButton" text="Go Back" />
   </children>
</AnchorPane>
